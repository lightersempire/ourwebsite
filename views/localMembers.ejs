    <!-- partial -->
    <div class="container-fluid page-body-wrapper">


      <div class="main-panel">
        <div class="content-wrapper pb-0">


          <!-- image card row starts here -->
          <div class="grey-bg container-fluid">
            <section id="minimal-statistics">
              <div class="row">
                <div class="col-12 mt-3 mb-1">
                  <h4 class="text-uppercase" style="color: #0073aa;">Local Members</h4>
                </div>
              </div>

              <div class="row">
                <div class="col-xl-12 col-sm-12 col-12">


                  <br><br>


                  <table id="example" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                      <tr>
                        <th>Last Name</th>
                        <th>First Name</th>
                        <th>Phone</th>
                        <th>Email</th>
                        <th>Country</th>
                        <th>State</th>
                        <th>comment</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% LocalMembers.forEach(function(member){ %>
                        <tr>
                          <td>
                            <%=member.lastname%>
                          </td>
                          <td>
                            <%= member.firstname%>
                          </td>
                          <td>
                            <%= member.phone%>
                          </td>
                          <td>
                            <%= member.email%>
                          </td>
                          <td>
                            <%= member.country%>
                          </td>
                          <td>
                            <%= member.state%>
                          </td>
                          <td>
                            <%= member.comment%>
                          </td>
                        </tr>
                        <%})%>
                    </tbody>

                  </table>





                </div>


              </div>
              <!-- table row starts here -->

              <!-- doughnut chart row starts -->

              <!-- last row starts here -->

          </div>

          <!-- partial -->
        </div>
